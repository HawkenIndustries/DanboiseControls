<permissions>
  <niagara-permission-groups type="all">
    <!-- Insert any global permissions here. -->
  </niagara-permission-groups>
  <niagara-permission-groups type="workbench">
    <!-- Insert any workbench specific permissions here. -->
  </niagara-permission-groups>
  <niagara-permission-groups type="station">
    <!-- Insert any station specific permissions here. -->
    <req-permission>
      <name>NETWORK_COMMUNICATION</name>
      <purposeKey>Smolinski.</purposeKey>
      <parameters>
        <parameter name='hosts' value='*'></parameter>
        <parameter name='ports' value='*'></parameter>
        <parameter name='type' value='all'></parameter>
      </parameters>
    </req-permission>
    <req-permission>
      <name>MANAGE_EXECUTION</name>
      <purposeKey>Smolinski</purposeKey>
    </req-permission>
    <req-permission>
      <name>SHUTDOWN_HOOKS</name>
      <purposeKey>Smolinski</purposeKey>
    </req-permission>
    <req-permission>
      <name>LOGGING</name>
      <purposeKey>Smolinski</purposeKey>
    </req-permission>
    <req-permission>
      <name>MODIFY_IO_STREAMS</name>
      <purposeKey>Smolinski</purposeKey>
    </req-permission>
    <req-permission>
      <name>SYSTEM_PROPERTIES</name>
      <purposeKey>Smolinski</purposeKey>
      <parameters>
        <parameter name='properties' value='*'></parameter>
        <parameter name='actions' value='read,write'></parameter>
      </parameters>
    </req-permission>
    <req-permission>
      <name>SET_SYSTEM_TIME</name>
      <purposeKey>Smolinski</purposeKey>
    </req-permission>
    <req-permission>
      <name>LOAD_LIBRARIES</name>
      <purposeKey>Smolinski</purposeKey>
      <parameters>
        <parameter name='libraries' value='*'></parameter>
      </parameters>
    </req-permission>
    <req-permission>
      <name>AUTHENTICATION</name>
      <purposeKey>Smolinski</purposeKey>
    </req-permission>
    <req-permission>
      <name>GET_ENVIRONMENT_VARIABLES</name>
      <purposeKey>Smolinski</purposeKey>
      <parameters>
        <parameter name='variables' value='NIAGARA_HOME,NIAGARA_USER_HOME,NIAGARA_DEV_HOME,GRADLE_HOME,JAVA_HOME,MAVEN_HOME,TEMP,TMP,WINDIR,PATH,CLASSPATH,COMSPEC,PATH,USERNAME,LOGONSERVER,USERDOMAIN,USERNAME,USERPROFILE,HOMEPATH,HOMEDRIVE,APPDATA,LOCALAPPDATA,CLIENTNAME'></parameter>
      </parameters>
    </req-permission>
    <req-permission>
      <name>REFLECTION</name>
      <purposeKey>Smolinski</purposeKey>
    </req-permission>
    <req-permission>
      <name>UI</name>
      <purposeKey>Smolinski</purposeKey>
    </req-permission>
    <req-permission>
      <name>RUNTIME_EXECUTION</name>
      <purposeKey>Smolinski</purposeKey>
      <parameters>
        <parameter name="files" value="*"></parameter>
      </parameters>
    </req-permission>
  </niagara-permission-groups>
</permissions>
